# 生成两维矩阵
lst = [[1, 2, 3]]
for i in range(2):
    pre = lst[i]
    cur = []
    for j in range(3):
        cur.append(pre[j] + 3)
    lst.append(cur)
print(lst)

print("====="*20)

# 横着打印
for i in range(3):
    for j in range(3):
        print(lst[i][j], end = " ")
    print()

print("====="*20)
    
# 竖着打印
for i in range(3):
    for j in range(3):
        print(lst[j][i], end = " ")
    print()
    